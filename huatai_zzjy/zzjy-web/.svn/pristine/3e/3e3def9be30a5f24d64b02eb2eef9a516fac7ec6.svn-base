	<!-- 按角色、主题、区域查询模板指标数据 -->
	<select id="findTarRegsBySubAndRegAndRole">
		select
		<include refid="Base_Column_List" />
		from ht_tar_reg_tb tar_reg
		left join ht_role_target_tb role_target
		on
		tar_reg.TARGET_CODE=role_target.TARGET_CODE
		where
		role_target.ROLE_CODE=#{role} AND
		tar_reg.REG_ID=#{regId} AND
		tar_reg.SUB_CODE=#{sub}
		AND tar_reg.OP_TYPE!='D'
		AND
		role_target.OP_TYPE!='D'
	</select>

	<!-- 按角色、主题查询模板指标数据 -->
	<select id="findTarRegsBySubAndRole" resultMap="BaseResultMap">
		SELECT
		tar_reg.TR_ID,
		tar_reg.REG_ID,
		tar_reg.TARGET_CODE,
		tar_reg.SUB_CODE,
		tar_reg.DIM_TYPE,
		tar_reg.GRAPH_TYPE,
		tar_reg.GRAPH_TITLE,
		tar_reg.X_NAME,
		tar_reg.Y_NAME,
		tar_reg.UNIT,
		tar_reg.ROLE_ORG_TYPE,
		tar_reg.IS_RANK,
		tar_reg.RANK_ORG_TYPE,
		tar_reg.IS_DISPLAY,
		tar_reg.SORT,
		tar_reg.PWD
		FROM ht_tar_reg_tb tar_reg
		LEFT JOIN ht_role_target_tb role_target
		ON tar_reg.TARGET_CODE =
		role_target.TARGET_CODE
		LEFT JOIN ht_tem_reg_tb tem_reg
		ON
		tem_reg.REG_ID = tar_reg.REG_ID
		LEFT JOIN ht_templet_tb templet
		ON
		templet.TEMP_ID = tem_reg.TEMP_ID
		LEFT JOIN ht_target_tb target
		ON
		target.TARGET_CODE=role_target.TARGET_CODE
		WHERE
		role_target.ROLE_CODE=#{role}
		AND
		tar_reg.SUB_CODE=#{sub}
		AND
		templet.TEMP_CODE=#{temp}
		AND target.TARGET_TYPE IN (1,3)
		AND
		tar_reg.OP_TYPE <![CDATA[ <>  ]]>
		'D'
		AND role_target.OP_TYPE <![CDATA[ <>  ]]>
		'D'
		AND tem_reg.OP_TYPE <![CDATA[ <>  ]]>
		'D'
		AND target.OP_TYPE <![CDATA[ <>  ]]>
		'D'
		ORDER BY tar_reg.TARGET_CODE ASC
	</select>

	<!-- 按角色、主题查询主题下区域 -->
	<select id="findRegionBySubAndRole" resultMap="BaseResultMap">
		select
		DISTINCT
		tar_reg.REG_ID,
		tar_reg.ROLE_ORG_TYPE
		from
		ht_tar_reg_tb tar_reg
		left join
		ht_role_target_tb
		role_target
		on
		tar_reg.TARGET_CODE=role_target.TARGET_CODE
		left join
		ht_tem_reg_tb
		tem_reg
		on tem_reg.REG_ID=tar_reg.REG_ID
		left join
		ht_templet_tb templet
		on templet.TEMP_ID=tem_reg.TEMP_ID
		where
		role_target.ROLE_CODE=#{role}
		AND
		tar_reg.SUB_CODE=#{sub}
		AND
		templet.TEMP_CODE=#{temp}
		AND
		tar_reg.OP_TYPE!='D'
		AND tem_reg.OP_TYPE!='D'
		AND
		role_target.OP_TYPE!='D'
	</select>

	<select id="findAll" resultMap="BaseResultMap" parameterType="com.huatai.web.model.TarReg">
		select
		<include refid="Base_Column_List" />
		from ht_tar_reg_tb
		<where>
			<if test="trId !=null and trId !=''">
				AND TR_ID=#{trId}
			</if>
			<if test="regId !=null and regId !=''">
				AND REG_ID=#{regId}
			</if>
			<if test="targetCode !=null and targetCode !=''">
				AND TARGET_CODE = #{targetCode}
			</if>
			<if test="opType !=null and opType !=''">
				AND OP_TYPE=#{opType}
			</if>
			<if test="opType ==null or opType ==''">
				AND OP_TYPE!='D'
			</if>
		</where>
		ORDER BY CREATE_TIME DESC
	</select>

	<select id="findAllLike" resultMap="BaseResultMap"
		parameterType="com.huatai.web.model.TarReg">
		select
		<include refid="Base_Column_List" />
		from ht_tar_reg_tb
		<where>
			<if test="trId !=null and trId !=''">
				AND TR_ID=#{trId}
			</if>
			<if test="regId !=null and regId !=''">
				AND REG_ID=#{regId}
			</if>
			<if test="targetCode !=null and targetCode !=''">
				AND TARGET_CODE like '%${targetCode}%'
			</if>
			<if test="opType !=null and opType !=''">
				AND OP_TYPE=#{opType}
			</if>
			<if test="opType ==null or opType ==''">
				AND OP_TYPE!='D'
			</if>
		</where>
	</select>

	<select id="findTarRegsByRegion" resultMap="BaseResultMap">
		select
		tar_reg.TR_ID,
		tar_reg.REG_ID,
		tar_reg.TARGET_CODE,
		tar_reg.SUB_CODE,
		tar_reg.DIM_TYPE,
		tar_reg.GRAPH_TYPE,
		tar_reg.GRAPH_TITLE,
		tar_reg.X_NAME,
		tar_reg.Y_NAME,
		tar_reg.UNIT,
		tar_reg.ROLE_ORG_TYPE,
		tar_reg.IS_RANK,
		tar_reg.RANK_ORG_TYPE,
		tar_reg.IS_DISPLAY,
		tar_reg.SORT,
		tar_reg.PWD
		from
		ht_tar_reg_tb tar_reg
		left join
		ht_role_target_tb role_target
		on
		tar_reg.TARGET_CODE=role_target.TARGET_CODE
		left join ht_target_tb
		target
		on tar_reg.TARGET_CODE=target.TARGET_CODE
		where
		role_target.ROLE_CODE=#{role}
		AND
		tar_reg.REG_ID=#{regId}
		AND
		target.DEPT_CODE=#{roleDept}
		AND
		tar_reg.OP_TYPE!='D'
		AND
		target.OP_TYPE!='D'
		AND
		role_target.OP_TYPE!='D'
		ORDER BY tar_reg.SORT
	</select>

	<select id="findTarRegsByRegionAndSubject" resultMap="BaseResultMap">
		select
		tar_reg.TR_ID,
		tar_reg.REG_ID,
		tar_reg.TARGET_CODE,
		tar_reg.SUB_CODE,
		tar_reg.DIM_TYPE,
		tar_reg.GRAPH_TYPE,
		tar_reg.GRAPH_TITLE,
		tar_reg.X_NAME,
		tar_reg.Y_NAME,
		tar_reg.UNIT,
		tar_reg.ROLE_ORG_TYPE,
		tar_reg.IS_RANK,
		tar_reg.RANK_ORG_TYPE,
		tar_reg.IS_DISPLAY,
		tar_reg.SORT,
		tar_reg.PWD
		from
		ht_tar_reg_tb tar_reg
		left join
		ht_role_target_tb role_target
		on
		tar_reg.TARGET_CODE=role_target.TARGET_CODE
		left join ht_target_tb
		target
		on tar_reg.TARGET_CODE=target.TARGET_CODE
		where
		role_target.ROLE_CODE=#{role}
		AND
		tar_reg.REG_ID=#{regId}
		AND
		tar_reg.SUB_CODE=#{subject}
		<if test="roleDept !=null and roleDept !=''">
			AND target.DEPT_CODE=#{roleDept}
		</if>
		AND
		tar_reg.OP_TYPE!='D'
		AND
		target.OP_TYPE!='D'
		AND
		role_target.OP_TYPE!='D'
		ORDER BY tar_reg.SORT
	</select>

	<!--获得区域图标等属性 -->
	<select id="getTarRegByCOC" parameterType="java.lang.String"
		resultType="com.huatai.web.bean.TargetDetailBean">
		SELECT TARGET_CODE as targetCode,GRAPH_TYPE as type,
		GRAPH_TITLE as title,T1.COLOR,
		UNIT, SORT, PWD FROM HT_TAR_REG_TB T1
		LEFT JOIN
		HT_TEM_REG_TB T2 ON T1.REG_ID = T2.REG_ID
		WHERE T1.TARGET_CODE
		=
		'${code}' AND T1.ROLE_ORG_TYPE &lt;= '${orgType}'
		AND T1.IS_DISPLAY =
		'1' AND T1.OP_TYPE NOT IN ('D') AND T2.REG_CODE =
		'${regCode}'
	</select>

	<!-- 按区域id和板块id查询区域指标数据 -->
	<select id="findPlateTarRegByRegAndPlate" resultMap="BaseResultMap">
		SELECT
		DISTINCT
		A.TARGET_CODE,
		A.SUB_CODE,
		A.ROLE_ORG_TYPE
		FROM
		ht_tar_reg_tb A
		RIGHT JOIN
		ht_plate_one_tar_tb B ON
		A.TARGET_CODE =
		B.TARGET_CODE
		WHERE
		B.PLATE_ID =
		#{blockId}
		AND A.REG_ID
		= #{regId}
		AND
		A.IS_DISPLAY =
		'1' AND
		A.OP_TYPE NOT
		IN ('D')
		AND B.OP_TYPE NOT
		IN ('D')
		UNION
		SELECT
		A.TARGET_CODE,
		A.SUB_CODE,
		A.ROLE_ORG_TYPE
		FROM
		ht_tar_reg_tb A
		RIGHT JOIN
		ht_plate_two_tar_tb B
		ON A.TARGET_CODE =
		B.TARGET_CODE
		LEFT JOIN
		ht_plate_two_tar_gro_tb C ON
		C.PTTG_ID =
		B.PTTG_ID
		WHERE
		C.PLATE_ID =
		#{blockId}
		AND A.REG_ID = #{regId}
		AND
		A.IS_DISPLAY =
		'1' AND A.OP_TYPE
		NOT IN ('D')
		AND B.OP_TYPE NOT
		IN ('D')
	</select>

	<select id="findMoreTarRegsBySubAndRole" resultMap="BaseResultMap">
		SELECT
		T8.TARGET_CODE,T8.TARGET_NAME ,T8.TR_ID,
		T8.REG_ID,T8.SUB_CODE,T8.DIM_TYPE,
		T8.GRAPH_TYPE,T8.GRAPH_TITLE,T8.X_NAME,
		T8.Y_NAME,T8.UNIT,T8.ROLE_ORG_TYPE,
		T8.IS_RANK,T8.RANK_ORG_TYPE,
		T8.IS_DISPLAY,T8.SORT,T8.PWD
		FROM
		(SELECT
		T.TARGET_CODE,
		T2.TARGET_NAME,
		T.TR_ID,
		T.REG_ID,
		T.SUB_CODE,
		T.DIM_TYPE,
		T.GRAPH_TYPE,
		T.GRAPH_TITLE,
		T.X_NAME,
		T.Y_NAME,
		T.UNIT,
		T.ROLE_ORG_TYPE,
		T.IS_RANK,
		T.RANK_ORG_TYPE,
		T.IS_DISPLAY,
		T.SORT,
		T.PWD
		FROM ht_tar_reg_tb T,
		HT_TARGET_TB T2,
		HT_ROLE_TARGET_TB T3
		WHERE T.REG_ID IN(SELECT
		T1.REG_ID
		FROM
		HT_TEM_REG_TB T1
		WHERE T1.REG_CODE IN
		<foreach collection="temps" item="item" index="index" open="("
			separator="," close=")">
			'${item}'
		</foreach>
		AND T1.OP_TYPE <![CDATA[ <> ]]>
		'D')
		AND T.TARGET_CODE = T2.TARGET_CODE
		AND T2.OP_TYPE <![CDATA[ <>  ]]>
		'D'
		AND T.OP_TYPE <![CDATA[ <>  ]]>
		'D'
		AND T2.TARGET_CODE = T3.TARGET_CODE
		AND T3.OP_TYPE <![CDATA[ <>  ]]>
		'D'
		AND T.SUB_CODE = '${subject}'
		AND T3.ROLE_CODE = '${role}'
		ORDER BY
		T.TARGET_CODE) T8
		JOIN HT_TAR_GRO_DIM_TB T10 ON T8.TARGET_CODE =
		T10.TARGET_CODE
		JOIN HT_GRO_DIM_DETAIL_TB T12 ON T10.GDD_ID =
		T12.GDD_ID
		WHERE
		T12.GRO_DIM_CODE = '${groupbyDate}'
		AND T12.OP_TYPE <![CDATA[ <>  ]]>
		'D'
		AND T10.OP_TYPE <![CDATA[ <>  ]]>
		'D'
	</select>

</mapper>