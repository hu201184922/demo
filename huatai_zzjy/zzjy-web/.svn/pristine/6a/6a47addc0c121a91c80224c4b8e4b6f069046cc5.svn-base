	<!-- 根据指标查询分组维度代码 -->
	<select id="findGroDetailCodeByTarget" resultMap="BaseResultMap">
		SELECT
		t3.GRO_DIM_CODE
		FROM
		(
		SELECT
		t1.*
		FROM
		ht_gro_dim_detail_tb t1
		LEFT JOIN
		ht_gro_dim_tb t2
		ON t1.GD_ID=t2.GD_ID
		WHERE
		t2.GRO_DIM_TYPE_CODE='ORG_GROUP'
		AND t2.OP_TYPE!='D'
		) t3
		RIGHT JOIN
		ht_tar_gro_dim_tb t4 ON
		t3.gdd_id = t4.gdd_id
		AND t4.target_code =
		#{target}
		ORDER BY
		t3.SORT
		DESC
		LIMIT 1
	</select>

	<!-- 根据指标查询分组维度代码 -->
	<select id="findGroDetailListByTarget" resultMap="BaseResultMap">
		SELECT DISTINCT
		A.GDD_ID,
		A.GD_ID,
		A.GRO_DIM_CODE,
		A.GRO_DIM_NAME
		FROM
		ht_gro_dim_detail_tb A
		LEFT JOIN ht_tar_gro_dim_tb B ON A.GDD_ID
		=B.GDD_ID
		LEFT JOIN ht_target_tb C ON C.TARGET_CODE = B.TARGET_CODE
		LEFT JOIN ht_gro_dim_tb D ON D.GD_ID= A.GD_ID
		WHERE
		D.GRO_DIM_TYPE_CODE=#{groupType}
		AND C.TARGET_CODE = #{target}
		AND
		A.OP_TYPE!='D'
		AND B.OP_TYPE!='D'
		AND C.OP_TYPE!='D'
		AND D.OP_TYPE!='D'
	</select>

	<!-- 按主题查询时间分组维度 -->
	<select id="findGroDetailListBySubWithDate" resultMap="BaseResultMap">
		SELECT
		DISTINCT
		A.GDD_ID,
		A.GRO_DIM_CODE,
		A.GRO_DIM_NAME
		FROM
		ht_gro_dim_detail_tb A
		LEFT JOIN ht_gro_dim_tb B ON A.GD_ID = B.GD_ID
		LEFT JOIN ht_tar_gro_dim_tb C ON C.GDD_ID = A.GDD_ID
		WHERE
		B.GRO_DIM_TYPE_CODE = 'DATE_GROUP'
		AND C.TARGET_CODE = #{subCode}
		AND
		A.OP_TYPE!='D'
		AND B.OP_TYPE!='D'
		AND C.OP_TYPE!='D'
		ORDER
		BY A.SORT ASC
	</select>

	<!-- 按主题查询自定义板块时间分组维度 -->
	<select id="findGroDetailListBySubWithDateAndPlate" resultMap="BaseResultMap">
		SELECT
		*
		FROM
		(
		SELECT DISTINCT
		A.GDD_ID AS GDD_ID,
		A.GRO_DIM_CODE AS
		GRO_DIM_CODE,
		A.GRO_DIM_NAME AS GRO_DIM_NAME,
		A.SORT AS SORT
		FROM
		ht_gro_dim_detail_tb A
		LEFT JOIN ht_gro_dim_tb B ON A.GD_ID = B.GD_ID
		LEFT JOIN
		ht_tar_gro_dim_tb C ON C.GDD_ID = A.GDD_ID
		LEFT JOIN
		ht_plate_one_tar_tb D ON D.TARGET_CODE = C.TARGET_CODE
		WHERE
		B.GRO_DIM_TYPE_CODE = 'DATE_GROUP'
		AND D.PLATE_ID = #{plateId}
		AND
		A.OP_TYPE!='D'
		AND B.OP_TYPE!='D'
		AND C.OP_TYPE!='D'
		AND D.OP_TYPE!='D'
		UNION
		SELECT DISTINCT
		A.GDD_ID AS GDD_ID,
		A.GRO_DIM_CODE AS GRO_DIM_CODE,
		A.GRO_DIM_NAME AS GRO_DIM_NAME,
		A.SORT AS SORT
		FROM
		ht_gro_dim_detail_tb
		A
		LEFT JOIN ht_gro_dim_tb B ON A.GD_ID = B.GD_ID
		LEFT JOIN
		ht_tar_gro_dim_tb C ON C.GDD_ID = A.GDD_ID
		LEFT JOIN
		ht_plate_two_tar_tb D ON D.TARGET_CODE = C.TARGET_CODE
		LEFT JOIN
		ht_plate_two_tar_gro_tb E ON E.PTTG_ID = D.PTTG_ID
		WHERE
		B.GRO_DIM_TYPE_CODE = 'DATE_GROUP'
		AND E.PLATE_ID = #{plateId}
		AND
		A.OP_TYPE!='D'
		AND B.OP_TYPE!='D'
		AND C.OP_TYPE!='D'
		AND D.OP_TYPE!='D'
		AND E.OP_TYPE!='D'
		) T
		WHERE
		1 = 1
		ORDER BY
		T.SORT ASC
	</select>

</mapper>