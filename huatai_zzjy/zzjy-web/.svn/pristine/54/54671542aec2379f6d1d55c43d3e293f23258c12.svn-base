  <select id="findTIByTarChanCode" parameterType="java.lang.String" resultType="com.huatai.web.bean.FieldInterBean">
  	 SELECT T3.FIELD_CODE as fieldCode,T2.INTER_NAME as interName FROM HT_TARGET_INTER_TB T1 
	  LEFT JOIN  HT_INTER_TB T2 ON T1.INTER_ID = T2.INTER_ID
	  LEFT JOIN HT_INTER_FIELD_TB T3 ON T2.INTER_ID = T3.INTER_ID  
	 WHERE T1.TARGET_CODE = SUBSTRING('${targetCode}',1,7) AND T3.IS_DIM = '1'
	 		AND T3.DIM_CODE = '${channelCode}'
	 		and T2.INTER_NAME = '${interName}'
  </select>
