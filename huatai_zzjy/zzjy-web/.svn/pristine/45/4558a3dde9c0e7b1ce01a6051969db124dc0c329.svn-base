/*我的历程*/
.share-timetree {
	position: relative;
	display: block;
	margin: 15px 10px;
}
.share-timetree >li {
	position: relative;
	display: block;
	width: 80%;
	padding:2px 10px 0 32px;
}
.share-timetree >li:before {
	content: "";
	position: absolute;
	left: 50px;
	top:0;
	bottom:0;
	width: 3px;
	background: #d5d5d5;
	z-index:1;
}
.share-timetree >li:after {
	content: "";
	position: absolute;
	left: 42px;
	top: 44%;
	width: 15px;
	height: 15px;
	border: solid 2px #ff8b74;
	border-radius: 50%;
	margin-top: -5px;
	z-index: 1;
	background:#ff8b74;
}
.share-timetree .tip4:after {border: solid 2px #8be3cb;background:#8be3cb;}
.share-timetree .tip3:after {border: solid 2px #b0e269;background:#b0e269;}
.share-timetree .tip2:after {border: solid 2px #ffd76d;background:#ffd76d;}
