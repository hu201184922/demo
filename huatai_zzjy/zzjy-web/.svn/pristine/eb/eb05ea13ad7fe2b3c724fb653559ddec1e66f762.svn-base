		parameterType="java.lang.String">
		SELECT
		<include refid="Base_Column_List" />
		from ht_fixed_list_tb f
		where OP_TYPE!='D' AND
		f.FL_DEPT_CODE=${flDeptCode}
	</select>

	<select id="findDeptCodeByGroup" resultMap="BaseResultMap">
		SELECT FL_DEPT_CODE
		FROM ht_fixed_list_tb
		WHERE
		OP_TYPE!='D'
		GROUP BY FL_DEPT_CODE
	</select>

	<!-- 按角色查询固定清单部门 -->
	<select id="findFixedListDeptByRole" resultMap="BaseResultMap">
		select
		fixed_list.FL_DEPT_CODE
		FROM
		ht_fixed_list_tb fixed_list
		LEFT JOIN
		ht_role_fixed_list_tb
		role_fixed_list
		ON
		fixed_list.FL_CODE=role_fixed_list.FL_CODE
		WHERE
		role_fixed_list.ROLE_CODE=#{role}
		AND
		fixed_list.OP_TYPE!='D'
		AND
		role_fixed_list.OP_TYPE!='D'
		GROUP BY fixed_list.FL_DEPT_CODE
	</select>

	<!-- 按角色查询固定清单部门 -->
	<select id="findFixedListByRoleAndDept" resultMap="BaseResultMap">
		select
		fixed_list.FL_CODE,
		fixed_list.FL_NAME,
		fixed_list.FL_DEPT_CODE
		FROM
		ht_fixed_list_tb fixed_list
		LEFT JOIN
		ht_role_fixed_list_tb
		role_fixed_list
		ON
		fixed_list.FL_CODE=role_fixed_list.FL_CODE
		WHERE
		role_fixed_list.ROLE_CODE=#{role}
		AND fixed_list.FL_DEPT_CODE=#{dept}
		AND
		fixed_list.OP_TYPE!='D'
		AND
		role_fixed_list.OP_TYPE!='D'
		ORDER BY
		fixed_list.FL_CODE
	</select>

	<!-- 按FlCode查询固定清单 -->
	<select id="findFixedByFlCode" parameterType="String" resultMap="BaseResultMap">
		SELECT
		*
		FROM
		ht_fixed_list_tb
		WHERE
		FL_CODE = #{flCode}
		AND OP_TYPE!='D'
