/*customer */
.person-img {
	width: 80px;
	height: 80px;
	border-radius: 40px;
}
.manage-tip {
	background: #fff;
	padding: 5px 15px 5px 5px;
	line-height: 30px;
	border: 1px solid #ddd;
	text-align: left;
	color: #ea2c2c;
	position: relative;
}
.manage-tip .right-btn {
	border: 1px solid #0e59a8;
	padding: 3px 8px;
	line-height: 18px;
	position: absolute;
	right: 15px;
	top: 7px;
	border-radius: 1.3rem;
}
.manage-tip .right-btn a {
	color: #0e59a8;
}
#letter {
	border-radius: 5px;
	font-size: 75px;
	color: #555;
	text-align: center;
	position: fixed;
	left: 45%;
	top: 45%;
	z-index: 99;
	display: none;
}
#letter img {
	width: 42px;
	height: 42px;
	float: left;
}
.sort_box {
	width: 100%;
	overflow: hidden;
}
.sort_list {
	position: relative;
	height: 80px;
	line-height: 22px;
	border-bottom: 1px solid #ddd;
	background: #fff;
}
.sort_list .num_logo {
	width: 50px;
	height: 50px;
	border-radius: 25px;
	overflow: hidden;
	position: absolute;
	top: 15px;
	left: 15px;
}
.sort_list .num_logo img {
	width: 50px;
	height: 50px;
}
.sort_list .num_name {
	color: #000;
	position: absolute;
	top: 13px;
	left: 70px;
	line-height: 28px;
}
.sort_list .telephone {
	color: #000;
	position: absolute;
	top: 13px;
	left: 140px;
	line-height: 28px;
}
.sort_list .telephone em, .sort_list .num_name em {
	color: #999;
	font-size: 13px;
}
.sort_list .telephone i {
	padding-left: 10px;
	color: #1ac612;
	cursor: pointer;
}
.sort_letter {
	height: 30px;
	line-height: 30px;
	padding-left: 15px;
	color: #000;
	border-bottom: 1px solid #ddd;
}
.initials {
	position: fixed;
	line-height: 23px;
	color: #0088ff;
	bottom: -5px;
	right: 0px;
	height: 100%;
	width: 15px;
	padding-right: 10px;
	text-align: center;
	font-size: 12px;
	z-index: 99;
	background: rgba(145,145,145,0);
}
/*用户详情 纪念日*/
.mt15 {
	margin-top: 15px !important
}
.anniversary ul:nth-child(3)::before {
	height: 0 !important
}
.anniversary ul:nth-child(5)::before {
	height: 0 !important
}
/*客户详情 &准客客户详情*/
.details_header {
	background-color: #fff;
	width: 100%;
	padding-top: 15px;
}
.details_header h1 {
	font-size: 19px;
	margin-top: 11px
}
.mt25 {
	margin-top: 25px !important
}
.detail_input .mui-input-group::before {
	height: 0
}
.data_remo {
	line-height: 40px;
	height: 40px;
	margin-right: 15px;
}
.data_remo span {
	font-size: 15px;
	color: #777777;
}
/*新增客户信息*/
.new_add_arr {
	line-height: 40px;
	height: 40px;
	margin-right: 15px
}
.new_details_header {
	background-color: #fff;
	width: 100%;
	padding-top: 15px;
}
.new_details_header h1 {
	font-size: 19px;
	margin-top: 11px;
	color: #333;
	font-weight: bold;
}
.new_details_header p {
	color: #cacaca!important
}
.new_detail_input label {
	color: #888888 !important
}
/*准/客户信息变更*/
.mui-alert {
	line-height: 40px;
	height: 40px
}
.mui-alert p:first-child {
	margin-top: 8px;
	margin-left: 15px;
	float: left;
}
.mui-alert p:last-child {
	float: left;
	color: #da2424;
	margin-left: 10px
}
.checkBoxTxt {
	margin-top: -30px;
	margin-right: 56px
}
.mt7 {
	margin-top: 7px !important
}
.mt24 {
	margin-top: 24px !important
}
.pr55 {
	padding-right: 55px !important
}
.alertTxt {
	height: 60px !important
}
.alertTxt label {
	color: #0e59a8;
	padding-top: 20px;
}
/*准客户详情*/
.data_remo_new {
	line-height: 40px;
	height: 40px;
	margin-right: 40px;
}
.detail_arrow {
	position: absolute;
	right: 10px;
	line-height: 40px;
	height: 40px
}
.mui-text-center img{
	width:80px;
	height:80px;
	border-radius:40px;	
}
.isdefault{
	top:9px !important;
}
.mui-pages{top:0px;}
.mui-selects{display:block; float:left;white-space:nowrap;text-overflow:ellipsis;overflow:hidden;}

.btn-submit.btn-disabled{color:#fff;border:1px solid #999;background-image: -webkit-linear-gradient(bottom, #999 10%, #f1f1f1 90%);}
.btn-submit{font-size:17px;color:#fff;border:1px solid #0e59a8;background-image: -webkit-linear-gradient(bottom, #0e59a8 10%, #3296ff 90%);width:50% !important;margin-left:auto;margin-right:auto;margin-top:10px;height:35px;padding:6px}
