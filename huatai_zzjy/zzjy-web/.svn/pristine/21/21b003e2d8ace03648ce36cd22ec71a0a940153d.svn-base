/* CSS Document */
/*公用样式*/
li{vertical-align:top;}
html, body, div, p, ul, li, dl, dt, dd, h1, h2, h3, h4, h5, h6, form, input, select, button, textarea, iframe, table, th, td { margin: 0; padding: 0;}
img { border: 0 none; vertical-align: middle; }
ul, ol {list-style: none outside none;}
ul, li { list-style-type: none outside none; }
h1, h2, h3, h4, h5, { font-size: 14px; }
h6{font-size: 12px;}
.checkbox{border:0;}
.yb_display{display:none;}
.button { cursor:pointer; width:50px; line-height:18px; text-align:center; color:#444; }
i, em, cite { font-style: normal;}
ul li{vertical-align:bottom; border:0px;  margin:0; padding:0; list-style:none;}
.clear{ clear:both;}
.fl{float:left;display:inline;}
.fr{float:right;display:inline;}
.tl{text-align:left;}

a{ color:#444; text-decoration:none; }
a:focus { -moz-outline-style: none; }

a {outline: none;}

