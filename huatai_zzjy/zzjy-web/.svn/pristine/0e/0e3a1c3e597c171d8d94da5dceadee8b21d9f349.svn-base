	<!-- 按模板和主题用户角色查询默认指标 -->
	<select id="findDefaultTarByTempAndSubAndUser" resultMap="BaseResultMap">
		SELECT
		DISTINCT
		A.SDT_ID,
		A.TARGET_CODE
		FROM ht_set_default_tar_tb A
		WHERE
		A.TEMP_CODE=#{temp}
		AND
		A.SUB_CODE=#{sub} AND
		A.USERNAME=#{username} AND
		A.ROLE_CODE=#{role} AND
		A.TARGET_GRO_CODE =#{level} AND
		A.OP_TYPE!='D'
	</select>
