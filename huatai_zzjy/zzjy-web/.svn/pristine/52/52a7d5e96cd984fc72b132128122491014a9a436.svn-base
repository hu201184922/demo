@charset "utf-8";
/* CSS Document */

body, ul, li, h1, h2, p {
	padding: 0;
	margin: 0;
	list-style-type: none;
	font-family: "\5fae\8f6f\96c5\9ed1", "Microsoft YaHei", Helvetica, Arial, sans-serif
}
img { max-width: 100% }
.container {
	width: 100%;
	height: 100%;
	position: absolute;
	left: 0;
	top: 0;
	/*overflow: hidden;*/
}
.nextBtn {
	position: fixed;
	-webkit-animation: next 1s 0s infinite;
	z-index: 30;
	right: -100%;
	bottom: 20%;
	width: 8rem;
	height: 3rem;
	line-height: 3rem;
	background: #000;
	opacity: 0.6;
	font-size: 1.4rem;
	border-radius: 5px 0 0 5px;
	color: #fff;
	text-align: center;
	text-decoration: none;
	-webkit-transition: right 1s 0s;
	transition: right 1s 0s
}
.page1, .page2 {
	width: 100%;
	height: 100%;
	font-size: 1.2rem;
	position: absolute;
	left: 0;
	top: 0
}
.page2 { background: none }
.circle_chart {
	height: 145px;
	position: absolute;
	left: 0;
	top: 0;
	width: 100%;
}
.line_chart {
	height: 265px;
	position: absolute;
	left: 0;
	top: 125px;
	width: 100%
}
.bottom {
	height: 15%;
	position: absolute;
	left: 0;
	width: 100%;
}
@media (min-height: 480px) {
  .bottom {    
	top: 380px;
  }
}
@media (min-height: 568px) {
  .bottom {    
	top: 380px;
  }
}
@media (min-height: 627px) {
  .bottom {    
	top: 420px;
  }
}
/*first screen  expense 消费  */
/*第一步*/

.color_get, .color_use {
	width: 100%;
	position: absolute;
	font-size: 1.2rem;
	left: 5%;
	top:4%;
	opacity: 0;
	z-index: 99;
	color:#333;
}
.color_use {
	left: 25%;
}
.color_get i { background: #08ad88 }
.color_use i { background: #d01a1a }
.color_get i, .color_use i {
	display: inline-block;
	width: .5rem;
	height: .5rem;
	border-radius: 100%;
	margin-right: 0.5rem
}
/*第二步*/
.ageLine i, .expenseLine i, .article_use i {
	width: 100%;
	height: 100%;
	position: absolute;
	right: 0;
	top: 0;
	background: #fff
}
.ageLine {
	background: url(../images/animation_04.png) no-repeat bottom center;
	z-index: 6
}
.expenseLine {
	background: url(../images/animation_05.png) no-repeat bottom center;
	z-index: 5
}
.expenseRest {
	background: url(../images/animation_10.png) no-repeat bottom center;
	z-index: 4
}
.expenseRest {
	width: 96%;
	left: 2%;
	opacity: 0
}
.expenseUse {
	color: #0f0f0f ;
	position: absolute;
	border:1px solid #ed450a;
	border-radius:20px;
	padding:3px 10px;
	right: 4%;
	bottom: 16%;
	opacity: 0;
	z-index: 7
}
.expenseGet {
	color: #0f0f0f;
	position: absolute;
	border:1px solid #ed450a;
	border-radius:20px;
	padding:3px 10px;
	left: 6%;
	top: 23%;
	margin-left: 1%;
	z-index: 8;
	opacity: 0
}
/*第三步*/
.article_map { background: url(../images/animation_01.png) no-repeat center bottom }
.article_mapLine {
	background: url(../images/animation_02.png) no-repeat center;
	opacity: 0
}
.article_map, .article_mapLine {
	background-size: 100% auto;
	width: 100%;
	height: 100%;
	top: 0%;
	right: 0%;
	position: absolute;
	z-index: 1;
}
.article_map i {
	width: 100%;
	height: 100%;
	position: absolute;
	right: 0;
	top: 0;
	background: #fff;
}
.article_use i { left: 0 }
.article_use {
	background: url(../images/animation_03.png) no-repeat center;
    position: absolute;
    right: 40%;
    bottom: 60%;
    width: 45%;
    z-index: 3;
    background-size: auto 45%;
    height: 55%;
}

/*第四步*/
.bottom_tit {
	background: url(../images/animation_15.png) no-repeat bottom center;
	background-size:100%;
	height:165px;
	width: 100%;
	text-align: center;
	position: absolute;
	top: 0;
	color: #666;
	opacity: 0;
	font-size: 1.8rem
}
.bottom_card {
	width: 22.5%;
	height: 100%;
	position: absolute;
	border-radius: 5px;
	text-align: center;
	font-size: 1.2rem;
	opacity: 0;
}
@media (min-height: 480px) {
  .bottom_card { 
	top:105%;
  }
}
@media (min-height: 568px) {
  .bottom_card {    
	top: 78%;
  }
}
@media (min-height: 627px) {
  .bottom_card {    
	top: 60%;
  }
}
.bottom_card i {
	display: block;
	width: 100%;
	height: 3rem;
	background: #90877c;
	margin:0;
	color:#fff;
	line-height: 3rem;
	font-size: 1.6rem
}
.bottom_card a{
	    display: block;
    border-radius: 50%;
    background: #65c383;
    height: 6rem;
    width: 6rem;
    color: #fff;
    margin: 8px auto;
    word-break: break-all;
    word-wrap: break-word;
    padding: 4px 0 0px;}
.card_1 { 
	left: 7%;	
	background: url(../images/animation_16.png) no-repeat bottom center;
	background-size:100%;
	}
.card_2 { 
	left: 30%;
	background: url(../images/animation_17.png) no-repeat bottom center;
	background-size:100%;
	}
.card_3 { 
	left: 51%;
	background: url(../images/animation_18.png) no-repeat bottom center;
	background-size:100%;
	}
.card_4 { 
	left: 69%;
	background: url(../images/animation_19.png) no-repeat bottom center;
	background-size:100%;
	}
.card_1 span, .card_2 span, .card_3 span, .card_4 span {
	width: 12px;
	height: 12px;
	display: block;
	background: #90877c;
	transform: rotate(45deg);
	-webkit-transform: rotate(45deg);
	-moz-transform: rotate(45deg);
	margin: -4px 0 0 46%;
	position: absolute;
}
.cardTxt_1 {
	background: url(../images/animation_06.png) no-repeat bottom center;
	z-index: 8
}
.cardTxt_2 {
	background: url(../images/animation_07.png) no-repeat bottom center;
	z-index: 8
}
.cardTxt_3 {
	background: url(../images/animation_08.png) no-repeat bottom center;
	z-index: 8
}
.cardTxt_4 {
	background: url(../images/animation_09.png) no-repeat bottom center;
	z-index: 8
}
.cardTxt_1, .cardTxt_2, .cardTxt_3, .cardTxt_4 { opacity: 0 }
.ageLine, .expenseLine, .expenseRest, .cardTxt_1, .cardTxt_2, .cardTxt_3, .cardTxt_4 {
	width: 100%;
	background-size: 100% auto;
	height: 265px;
	left: 0;
	top: 0;
	bottom: 0px;
	position: absolute
}
/*first screen movie*/

.go .color_get { -webkit-animation: p1Color 1s 0s }
.go .color_use { -webkit-animation: p2Color 1s 0.5s }
.go .ageLine i { -webkit-animation: ageLine 1.5s 1s }
.go .expenseUse { -webkit-animation: expenseUse 1s 2.5s }
.go .expenseLine i { -webkit-animation: expenseLine 1s 4s }
.go .expenseGet { -webkit-animation: expenseGet 1s 4.5s }
.go .expenseRest { -webkit-animation: san 1s 6.5s }
.go .article_use i { -webkit-animation: circleUse 0.5s 7.5s }
.go .article_map i { -webkit-animation: map 0.5s 8s }
.go .article_mapLine { -webkit-animation: mapLine 0.5s 8.5s }
.go .bottom_tit { -webkit-animation: botTit 0.5s 9.5s }
.go .card_1 { -webkit-animation: turn 0.5s 11.5s }
.go .cardTxt_1 { -webkit-animation: san 0.5s 11s }
.go .card_2 { -webkit-animation: turn 0.5s 13.5s }
.go .cardTxt_2 { -webkit-animation: san 0.5s 13s }
.go .card_3 { -webkit-animation: turn 0.5s 15.5s }
.go .cardTxt_3 { -webkit-animation: san 0.5s 15s }
.go .card_4 { -webkit-animation: turn 0.5s 17.5s }
.go .cardTxt_4 { -webkit-animation: san 0.5s 17s }
.over .bottom_tit { -webkit-animation: titHide 0.5s 0s }
.over .card_1 { -webkit-animation: hide_1 0.5s 0.5s }
.over .card_1 span { -webkit-animation: hide_1 0.5s 0.5s }
.over .card_2 { -webkit-animation: hide_2 0.5s 0.7s }
.over .card_3 { -webkit-animation: hide_3 0.5s 0.9s }
.over .card_4 { -webkit-animation: hide_4 0.5s 1.1s }
.over .ageLine i, .over .expenseLine i, .over .article_use i { opacity: 0 }
.over .color_get, .over .color_use, .over .expenseUse, .over .expenseGet, .over .expenseRest, .over .bottom_tit, .over .bottom_card, .over .article_map, .over .article_mapLine, .over .article_useTxt, .over .cardTxt_1, .over .cardTxt_2, .over .cardTxt_3, .over .cardTxt_4 { opacity: 1 }
.over .article_use, .over .article_useTxt {
	opacity: 0;
	display: none;
}
/*second screen*/
.lineUse2 {
	background: url(../images/animation_12.png) no-repeat top center;
	z-index: 10
}
.lineUseMap2 {
	background: url(../images/animation_11.png) no-repeat bottom center;
	z-index: 11;
	top: -25px !important;
}
.lineUse2, .lineUseMap2 {
	width: 100%;
	background-size: 100% auto;
	/*height: 220px;*/
	left: 0;
	top: inherit;
	position: absolute;
	bottom: 0px;
	opacity: 0
}
.lineUse2 { top: 0% }
.lineSick2 {
	left: 49%;
	margin-top:15%;
}
.lineAcc2 {
	right: 28%;
	margin-top:15%;
}
.lineSick2, .lineAcc2 {
	color: #4d90ff;
	font-size: 1.4rem;
	position: absolute;
	z-index: 11;
	opacity: 0
}
.lineStop2 {
	color: #0f0f0f;
	position: absolute;
	border:1px solid #ed450a;
	border-radius:20px;	
	padding:3px 10px;
	right: 23%;
	top: 49%;
	font-size: 1.4rem ;
	z-index: 12;
	opacity: 0;
	background:#fff;
}
.bottom_txt2_1, .bottom_txt2_2 {
	width: 100%;
	opacity: 0;
}
.bottom_txt2_1 {
	background: url(../images/animation_14.png) no-repeat center top;
	background-size: 100%;
	height: 85px;
	margin-top:10%;
}
.bottom_txt2_2 {
	background: url(../images/animation_13.png) no-repeat 0 0;
	background-size: 100%;
	height: 120px;
}
/*second screen movie*/
.topcontainer .circle_chart .article_map i{ background:none;}
.topcontainer .line_chart { -webkit-animation: p2Hiden 1s 1.6s }
.topcontainer .bottom { -webkit-animation: p2Top 1s 2.6s }
.topcontainer .circle_chart { -webkit-animation: p2None .2s 1.6s }
.go .lineSick2 { -webkit-animation: lineAcc 0.2s 2s }
.go .lineUse2 { -webkit-animation: lineUse 1s 3s }
.go .lineAcc2 { -webkit-animation: lineAcc 0.2s 2.5s }
.go .lineUseMap2 { -webkit-animation: lineUseMap 1s 3.5s }
.go .lineStop2 { -webkit-animation: lineStop 1s 4s }
.go .bottom_txt2_1 { -webkit-animation: p2Text 1s 5s }
.go .bottom_txt2_2 { -webkit-animation: p2Text 1s 5.5s }
.over .lineUse2, .over .lineUseMap2, .over .lineSick2, .over .lineAcc2, .over .lineStop2, .over .bottom_txt2_1, .over .bottom_txt2_2 { opacity: 1 }
