	<select id="isHasGroByTarAndGro" resultMap="BaseResultMap"
		parameterType="java.lang.String">
		SELECT
		TARGET_CODE
		FROM
		ht_tar_gro_dim_tb a
		LEFT JOIN
		ht_gro_dim_detail_tb b
		ON
		a.GDD_ID = b.GDD_ID
		WHERE
		TARGET_CODE =
		#{targetCode,jdbcType=VARCHAR}
		AND
		b.GRO_DIM_CODE =
		#{groupbyDate,jdbcType=VARCHAR};
	</select>

	<select id="getCommonOrgType" resultMap="BaseResultMap">
		SELECT
		A.GDD_ID
		FROM
		ht_tar_gro_dim_tb A
		LEFT JOIN
		ht_gro_dim_detail_tb B
		ON
		A.GDD_ID =
		B.GDD_ID
		LEFT JOIN
		ht_gro_dim_tb C
		ON
		C.GD_ID=B.GD_ID
		WHERE
		TARGET_CODE
		=#{targetCode}
		AND C.GRO_DIM_TYPE_CODE = #{groupType}
		AND A.OP_TYPE!='D'
		AND B.OP_TYPE!='D'
		AND C.OP_TYPE!='D'
		ORDER BY
		A.GDD_ID
		ASC
	</select>

