table{ background: #fff; }
.table>thead>tr>th { font-weight: normal; line-height: 35px; }
.table>tbody>tr>td { font-weight: normal; line-height: 35px; }
.table>tbody>tr>td>a{ color:#6babe5;}
.table-bordered>thead>tr>th, .table-bordered>tbody>tr>th, .table-bordered>tfoot>tr>th, .table-bordered>thead>tr>td, .table-bordered>tbody>tr>td, .table-bordered>tfoot>tr>td { border-left: none; border-right: none; padding-left:20px; }


.nav-tabs { border-bottom:none;}
.nav-tabs>li { float: left; margin-bottom: -1px }
.nav-tabs>li>a { margin:0px 10px 0px 0px; padding:5px 15px;  line-height: 1.42857143; border: 1px solid transparent; border-radius: 0; background:#eee; }
.nav-tabs>li>a:hover { border-color:#6babe5; background:#6babe5; color:#fff; }
.nav-tabs>li.active>a, .nav-tabs>li.active>a:hover, .nav-tabs>li.active>a:focus { color: #fff; cursor: default; background-color: #6babe5; border: 1px solid #ddd; border-bottom-color: transparent }
.nav-tabs.nav-justified { width: 100%; border-bottom: 0 }
.nav-tabs.nav-justified>li { float: none }
.nav-tabs.nav-justified>li>a { margin-bottom: 5px; text-align: center }
.nav-tabs.nav-justified>.dropdown .dropdown-menu { top: auto; left: auto }


.form-horizontal .control-label { font-weight: normal; margin:0px; vertical-align:middle; }
.form-control { margin: 0px; border-radius: 0px;  color:#999; font-weight:normal;  }
.form-control:focus { border-color: #ddd; outline: 0; -webkit-box-shadow: inset 0 1px 1px rgba(0,0,0,.075), 0 0 8px rgba(255,255,255,.6); box-shadow: inset 0 1px 1px rgba(0,0,0,.075), 0 0 8px rgba(255,255,255,.6) }
.pagination{ margin:0px;}