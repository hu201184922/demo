package com.fairyland.jdp.framework.tags.model;

import java.util.List;

import com.fairyland.jdp.framework.menu.view.MenuModel;

public class Navigation extends UIBean {
	public static final String OPEN_TEMPLATE = "nav.ftl";
	public static final String TEMPLATE = "nav-close.ftl";
	private List<MenuModel> menus;

	public String getDefaultOpenTemplate() {
		return "nav.ftl";
	}

	public String getDefaultTemplate() {
		return "nav-close.ftl";
	}

	public List<MenuModel> getMenus() {
		return this.menus;
	}

	public void setMenus(List<MenuModel> menus) {
		this.menus = menus;
	}
}