	<!-- 按主题查询接口字段 -->
	<select id="findInterFieldBySub" resultMap="BaseResultMap">
		SELECT
		DISTINCT
		inter_field.FIELD_CODE,
		inter_field.FIELD_NAME,
		inter_field.FIELD_TYPE
		FROM
		ht_inter_field_tb inter_field
		LEFT JOIN
		ht_inter_tb inter
		ON
		inter.INTER_ID=inter_field.INTER_ID
		LEFT JOIN
		ht_target_inter_tb
		target_inter
		ON target_inter.INTER_ID=inter.INTER_ID
		WHERE
		target_inter.TARGET_CODE=#{subject}
		AND inter_field.IS_DIM='1'
		AND
		inter_field.DIM_CODE=#{dimCode}
		AND inter_field.OP_TYPE!='D'
		AND
		inter.OP_TYPE!='D'
		AND target_inter.OP_TYPE!='D'
	</select>

