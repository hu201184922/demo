#banner #ifocus_piclist {z-index: 1; position: absolute}
#banner #ifocus_piclist li {width: 666x; height: 210px; overflow: hidden}
#banner #ifocus_piclist img {width: 666px; height: 210px}
#banner #ifocus_btn {z-index: 3; position: absolute; padding-left: 7px; width: 666px; bottom: 2px; height: auto; left: 0px; _bottom: 2px}
#banner #ifocus_btn li {filter: alpha(opacity=50); padding-bottom: 0px; margin-top: 10px; padding-left: 3px; width: auto; padding-right: 3px; display: block; float: left; height: auto; cursor: pointer; padding-top: 0px; opacity: 0.5; -moz-opacity: 0.5}
