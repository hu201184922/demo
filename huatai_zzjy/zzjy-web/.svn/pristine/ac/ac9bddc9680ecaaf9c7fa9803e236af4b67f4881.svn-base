##################################shiro配置#############################
#退出跳转链接配置
spring.fairyland.shiro.redirectUrl=/admin/login
#shiro总过滤器链名称配置
spring.fairyland.shiro.shiroFilter=adminShiroFilter
#spring管理生命周期配置（true为spring管理）
spring.fairyland.shiro.targetFilterLifecycle=true
#标志显示fiter启用不启用（true表示已经启用）
spring.fairyland.shiro.registrationEnabled=true
#adminShiroFilter拦截的链接配置(若有需配置多个，以换行符"/n"切开)
spring.fairyland.shiro.urlPatterns=/admin/*
#shiro的session管理器中定义会话超时时间配置
spring.fairyland.shiro.globalSessionTimeout=1800000
#shiro的session管理器中是否删除过期session的配置（true为删除）
spring.fairyland.shiro.deleteInvalidSessions=true
#shiro中是否开启扫描session会话线程
spring.fairyland.shiro.sessionValidationSchedulerEnabled=true
#是否启用/禁用Session Id Cookie,默认是启用的;如果禁用后将不会设置Session Id Cookie,即默认使用了Servlet容器的JSESSIONID
spring.fairyland.shiro.sessionIdCookieEnabled=true
#SimpleCookie的名称配置
spring.fairyland.shiro.fairylandSimpleCookieName=sid
spring.fairyland.shiro.simpleCookiehttpOnly=true
spring.fairyland.shiro.simpleCookiemaxAge=180000
#shiro的session超时时间设置
spring.fairyland.shiro.sessionValidationInterval=1800000
#shiro的加密配置
spring.fairyland.shiro.base64Encoded=4AvVhmFLUs0KTA3Kprsdag==
spring.fairyland.shiro.fairylandRememberMeCookieName=rememberMe
spring.fairyland.shiro.rememberMeCookiehttpOnly=true
spring.fairyland.shiro.rememberMeCookiemaxAge=604800
spring.fairyland.shiro.loginUrl=/admin/login
spring.fairyland.shiro.successUrl=/admin/desktop
spring.fairyland.shiro.unauthorizedUrl=/admin/unauthorized
spring.fairyland.shiro.failureKeyAttribute=error
spring.fairyland.shiro.lockedDuration=60000
spring.fairyland.shiro.passwordAttemptTimes=3
spring.fairyland.shiro.excessiveAttemptsPolicy=CAPTCHA
#shiro权限过滤链（配置多项需要加换行符“\n"）
spring.fairyland.shiro.filterChainDefinitions=\
/health=anon\n\
/admin/forgetpwd=anon\n\
/admin/cas_error =anon\n\
/admin/casLogin=anon\n\
/admin/login=anon,authc\n\
/admin/logout = logout\n\
/static/** = anon\n\
/api/**=anon\n\
/admin/unauthorized =anon\n\
/admin/permloading =anon\n\
/admin/sec/captcha = anon\n\
/admin/**=session,user
spring.fairyland.shiro.authcKeyAttribute=authc
spring.fairyland.shiro.logoutKeyAttribute=logout
spring.fairyland.shiro.sessionKeyAttribute=session
spring.fairyland.shiro.activeSessionsCacheName=shiro-activeSessionCache
spring.fairyland.shiro.hashAlgorithmName=SHA-1
spring.fairyland.shiro.hashIterations=1024
spring.fairyland.shiro.generatePublicSalt=true
spring.fairyland.shiro.proxyTargetClass=true
#ehcache文件路径配置
spring.fairyland.shiro.cacheManagerConfigFile=classpath:ehcache/ehcache-shiro.xml
spring.fairyland.shiro.setStaticMethod=org.apache.shiro.SecurityUtils.setSecurityManager
#bizShiroFilter
