<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ehuatai.biz.organization.mapper.OrgMapper">
	<select id="findOrgByRoleOrg" resultType="com.ehuatai.biz.organization.domain.Organization">
		SELECT DISTINCT
		TEAMCOMSHORTNAME,MANAGECOMCODE,MANAGECOMNAME,PROVCOMNAME,PROVCOMCODE
		FROM
		D_ORGANIZATION
		WHERE length(MANAGECOMCODE)= #{len}
		<if test="roleOrg != '' and roleOrg !=null ">
			AND PROVCOMCODE = #{roleOrg}
		</if>
	</select>
</mapper>