  <select id="findAll" resultMap="BaseResultMap">
  		SELECT
		a.ACCESSID,
		a.ACCESSNAME,
		a.ACCESSURL,
		a.BEHAVIORID,
		a.USER_ID,
		a.DATE_CODE,
		a.FM_NAME,
		a.ACCESS_TIME,
		a.OUT_TIME,
		a.IS_OUT,
		a.RES_TIME
	FROM
		access_page_info a,
		user_behavior_info u
		<where>
			a.behaviorid = u.behaviorid AND (a.OUT_TIME !='' or a.OUT_TIME !=null)
			<if test="orgs != null and orgs != ''">
				and u.LEVEL like '${orgs}%'
			</if>
			<if test="userId != null and userId != ''">
				and u.USER_ID = #{userId}
			</if>
			<if test="module != null and module != ''">
				and a.FM_NAME = #{module}
			</if>
			<if test="isNode != null and isNode != ''">
				and a.ACCESSNAME = #{isNode}
			</if>
			<if test="datetime1 != null and datetime1 != ''">
				and a.DATE_CODE  &gt;= #{datetime1}
			</if>
			<if test="datetime2 != null and datetime2 != ''">
				and a.DATE_CODE &lt;= #{datetime2}
			</if>
		</where>
  </select>
  <select id="findFunanList" resultType="com.huatai.web.bean.ModuleBean">
		SELECT
			accessName name,
			fm_name pName,
			COUNT(accessName) visitNum,
			COUNT(accessName) vnRatio,
			count(DISTINCT user_id) visitPeo,
			count(DISTINCT user_id) vpRatio,
			ROUND(sum((out_time - access_time)/100),2) stayTime,
			ROUND(sum((out_time - access_time)/100),2) stRatio,
			sum(CASE WHEN is_out = 1 THEN 1 END) outRatio,
			sum(CASE WHEN is_out = 0 THEN 1 END) convertRatio
		FROM
			access_page_info
		<where>
			<if test="dateDim!=null and dateDim!=''">
				DATEDIFF(DATE_CODE,#{dateDim}) &gt; 0
			</if>
		</where>
		GROUP BY
			accessName
  </select>
