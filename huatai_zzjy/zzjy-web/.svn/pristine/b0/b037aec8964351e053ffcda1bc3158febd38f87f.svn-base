	<select id="selectGroDimByTars" parameterType="java.lang.String"
		resultType="com.huatai.web.model.GroDimCodeStr">
		select a.GRO_DIM_TYPE_Code as
		groDimType,group_concat(b.GRO_DIM_CODE) as
		groDCodeStr
		from
		ht_gro_dim_tb a
		left join ht_gro_dim_detail_tb b
		on a.GD_ID = b.GD_ID
		left join ht_tar_gro_dim_tb c
		on b.gdd_id = c.GDD_ID
		where a.OP_TYPE !=
		'D'
		AND B.OP_TYPE != 'D'
		AND C.OP_TYPE != 'D'
		AND C.TARGET_CODE
		IN(${tarsStr})
		group by a.GRO_DIM_TYPE_Code
	</select>
	<select id="getTarListByQid" resultType="com.huatai.web.model.BillTargetDetail">
		SELECT DISTINCT
		a.bill_id AS billId, a.bill_name as billName,a.remark,
		b.SORT AS sort,
		b.FORMULA_TYPE AS formulaType, b.min_val AS minVal,
		b.max_val AS
		maxVal, b.TARGET_CODE AS targetCode, c.FILT_DIM_CODE AS
		filtDimCode
		FROM ht_bill_tb a, ht_bill_target_tb b, ht_bill_filt_dim_tb c
		WHERE
		a.BILL_ID = b.BILL_ID AND b.BILL_ID = c.BILL_ID AND b.TARGET_CODE =
		c.TARGET_CODE
		AND a.OP_TYPE != 'D' AND b.OP_TYPE != 'D' AND c.OP_TYPE
		!= 'D' AND
		a.bill_id = '${qid}'
		AND a.bill_type = 1
	</select>
	<select id="selectDeptInfo" resultType="com.huatai.web.model.DeptInfo">
		select b.code_ as
		deptCode, b.name_ as deptName, '0' as pid
		from jdp_dict a
		left join
		jdp_dict_item b
		on a.cid = b.DICT_ID
		where a.code_ = 'DETP_DICT'
	</select>

	<!-- 按用戶查询清单列表 -->
	<select id="findBillByPage" resultMap="BaseResultMap">
		SELECT
		<include refid="Base_Column_List"></include>
		FROM ht_bill_tb
		<where>
			ROLE_CODE=#{role}
			AND USERNAME =#{username}
			AND
			OP_TYPE!='D'
			<if test="billname != null and billname!=''">
				AND BILL_NAME LIKE CONCAT('%', #{billname},'%')
			</if>
		</where>
		ORDER BY CREATE_TIME DESC
	</select>
