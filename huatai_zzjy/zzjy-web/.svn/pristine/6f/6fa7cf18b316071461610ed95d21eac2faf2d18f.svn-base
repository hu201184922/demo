h4{font-weight:normal;padding-bottom:10px;color:#333;}
h4 button{color:#ff9c00;line-height:18px;padding:0 3px;border:1px solid #ff9c00;margin-left:6px;}
.mui-table-cell-delect{position:absolute;right:55px;color:#f00;}
.mui-table-cell-refuse{position:absolute;right:41px;color:#f00;}
.mui-table-cell-accept{position:absolute;right:0;color:#f00;}
.mui-h5 button{border-radius:5px;padding:3px;}
.num_name button{color:#ff9c00;line-height:18px;padding:0 3px;margin-top:3px;border:1px solid #ff9c00;margin-left:6px;}
/*customer */
.person-img {
	width: 80px;
	height: 80px;
	border-radius: 40px;
}
.manage-tip {
	background: #fff;
	padding: 5px 15px 5px 5px;
	line-height: 30px;
	border: 1px solid #ddd;
	text-align: left;
	color: #ea2c2c;
	position: relative;
}
.manage-tip .right-btn {
	border: 1px solid #0e59a8;
	padding: 3px 8px;
	line-height: 18px;
	position: absolute;
	right: 15px;
	top: 7px;
	border-radius: 1.3rem;
}
.manage-tip .right-btn a {
	color: #0e59a8;
}
#letter {
	border-radius: 5px;
	font-size: 75px;
	color: #555;
	text-align: center;
	position: fixed;
	left: 45%;
	top: 45%;
	z-index: 99;
	display: none;
}
#letter img {
	width: 42px;
	height: 42px;
	float: left;
}
.sort_box {
	width: 100%;
	overflow: hidden;
}
.sort_list {
	position: relative;
	height: 80px;
	line-height: 22px;
	border-bottom: 1px solid #ddd;
	background: #fff;
}
.sort_list .num_logo {
	width: 50px;
	height: 50px;
	border-radius: 25px;
	overflow: hidden;
	position: absolute;
	top: 15px;
	left: 15px;
}
.sort_list .num_logo img {
	width: 50px;
	height: 50px;
}
.sort_list .num_name {
	color: #000;
	position: absolute;
	top: 13px;
	left: 70px;
	line-height: 28px;
}
.sort_list .telephone {
	color: #000;
	position: absolute;
	top: 13px;
	left: 130px;
	line-height: 28px;
}
.sort_list .telephone em, .sort_list .num_name em {
	color: #999;
	font-size: 13px;
}
.sort_list .telephone i {
	padding-left: 10px;
	color: #1ac612;
	cursor: pointer;
}
.sort_letter {
	height: 30px;
	line-height: 30px;
	padding-left: 15px;
	color: #000;
	border-bottom: 1px solid #ddd;
}
.initials {
	position: fixed;
	line-height: 23px;
	color: #0088ff;
	bottom: -5px;
	right: 0px;
	height: 100%;
	width: 15px;
	padding-right: 10px;
	text-align: center;
	font-size: 12px;
	z-index: 99;
	background: rgba(145,145,145,0);
}

.plan-list-head-select{line-height:44px;font-size:17px;padding-left:15px;}


/*popup*/
.bgc{width: 110px;height: 60px;background-color: #000;opacity: 0.5;position: fixed;top: 60px;right: 0;}
.arrow-up{position:fixed;border-left: 8px solid transparent;border-right: 8px solid transparent;border-bottom: 15px solid rgba(0,0,0,.5);top:45px;right: 15px;}
.pop-txt{position: absolute;z-index: 100;top:62px;right:5px;color: #fff;font-size: 15px}
.pop-txt li:first-child{border-bottom: solid #C8C7CC 1px;line-height: 30px;height: 30px;}
.pop-txt li:last-child{line-height: 30px;height: 30px;}
.toggle{}