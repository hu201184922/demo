#banner #ifocus_btn .current {text-align: left; filter: alpha(opacity=100); margin-top: 0px; display: block; float: left; opacity: 1; -moz-opacity: 1}
#banner #ifocus_btn .current img {border-bottom: #fff 2px solid; border-left: #fff 2px solid; width: 111px; height: 35px; border-top: #fff 2px solid; border-right: #fff 2px solid}
#banner #ifocus_opdiv {z-index: 2; position: absolute; filter: alpha(opacity=50); width: 666px; bottom: 0px; background: #000; height: 40px; left: 0px; opacity: 0.5; -moz-opacity: 0.5; _height: 40px}
