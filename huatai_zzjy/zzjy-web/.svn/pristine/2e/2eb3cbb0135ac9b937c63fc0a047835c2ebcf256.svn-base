	<select id="findSubPlates" resultType="com.huatai.web.model.Target">
		SELECT 
			target_code as targetCode,
			target_name as targetName
		FROM 
			ht_target_tb 
		WHERE
			OP_TYPE!='D' 
		AND 
			target_type = 0 
		AND 
			is_plate in (0,1)
	</select>
	<select id="getTarBySubId" parameterType="java.lang.String"
		resultType="com.huatai.web.model.Target">
		SELECT 
			target_code as targetCode,
			target_name as targetName
		FROM 
			ht_target_tb 
		WHERE 
			OP_TYPE != 'D' 
		AND 
			IS_PLATE IN ('0','1') 
		AND
			SUBSTR(target_code,1,3) = '${subPlaId}' 
		AND 
			target_type IN (1,3)
	</select>
	<select id="findFuncList" resultType="com.huatai.web.model.DictItem">
		select 
			b.code_ as code,
			b.name_ as name 
		from 
			jdp_dict a 
		left join
			jdp_dict_item b
		on 
			a.cid =	b.dict_id 
		where 
			a.code_ = 'FUNC_TYPE'
	</select>
	<select id="findByPager" resultMap="BaseResultMap">
		select
			<include refid="ext_Column_List"></include>
		from 
			ht_tar_init_sql_tb i
		left join
			ht_target_tb t 
		on
			i.TARGET_CODE = t.TARGET_CODE
		left join 
			ht_gro_dim_detail_tb g 
		ON
			g.GRO_DIM_CODE = i.GROUP_DETAIL_CODE
		<where>
			t.OP_TYPE !='D' AND i.OP_TYPE !='D'
			<if test="tisId!=null and tisId!=''">
				and i.TIS_ID = #{tisId,jdbcType=INTEGER}
			</if>
			<if test="targetCode!=null and targetCode!=''">
				and i.TARGET_CODE = #{targetCode,jdbcType=VARCHAR}
			</if>
			<if test="groupType!=null and groupType!=''">
				and i.GROUP_TYPE = #{groupType,jdbcType=VARCHAR}
			</if>
			<if test="groupDetailCode!=null and groupDetailCode!=''">
				and i.GROUP_DETAIL_CODE =
				#{groupDetailCode,jdbcType=VARCHAR}
			</if>
			<if test="dateType!=null and dateType!=''">
				and i.DATE_TYPE = #{dateType,jdbcType=VARCHAR}
			</if>
			<if test="funIds!=null and funIds!=''">
				and i.FUN_IDS like '%${funIds}%'
			</if>
			<if test="sqlCode!=null and sqlCode!=''">
				and i.SQL_CODE = #{sqlCode,jdbcType=VARCHAR}
			</if>
			<if test="opType!=null and opType!=''">
				and i.OP_TYPE = #{opType,jdbcType=CHAR}
			</if>
			<if test="createTime!=null and createTime!=''">
				and i.CREATE_TIME = #{createTime,jdbcType=TIMESTAMP}
			</if>
			<if test="modifyTime!=null and modifyTime!=''">
				and i.MODIFY_TIME = #{modifyTime,jdbcType=TIMESTAMP}
			</if>
			<if test="creatorId!=null and creatorId!=''">
				and i.CREATOR_ID = #{creatorId,jdbcType=VARCHAR}
			</if>
			<if test="modifierId!=null and modifierId!=''">
				and i.MODIFIER_ID = #{modifierId,jdbcType=VARCHAR}
			</if>
		</where>
		ORDER BY CREATE_TIME DESC
	</select>

	<select id="getTarInitSql" resultMap="BaseResultMap">
		SELECT
		TARGET_CODE,
		GROUP_TYPE,
		GROUP_DETAIL_CODE,
		DATE_TYPE,
		FUN_IDS,
		UNIT,
		SQL_CODE
		FROM
		ht_tar_init_sql_tb
		WHERE
		FUN_IDS=#{fun}
		AND
		TARGET_CODE=#{targetCode}
		<if test="groupCode !=null and groupCode !=''">
			AND GROUP_TYPE=#{groupType}
		</if>
		<if test="groupCode ==null or groupCode ==''">
			AND (GROUP_TYPE IS NULL OR GROUP_TYPE='')
		</if>
		<if test="groupDetail !=null and groupDetail !=''">
			AND GROUP_DETAIL_CODE=#{groupDetail}
		</if>
		<if test="groupDetail ==null or groupDetail ==''">
			AND (GROUP_DETAIL_CODE IS NULL OR GROUP_DETAIL_CODE='')
		</if>
		<if test="dateType !=null and dateType !=''">
			AND DATE_TYPE=#{dateType}
		</if>
		<if test="dateType ==null or dateType ==''">
			AND (DATE_TYPE IS NULL OR DATE_TYPE='')
		</if>
		AND OP_TYPE!='D'
	</select>

