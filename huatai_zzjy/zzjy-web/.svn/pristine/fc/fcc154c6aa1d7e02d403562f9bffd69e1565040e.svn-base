/*share-timetree*/
.share-timetree {
	position: relative;
	display: block;
	margin: 10px 10px 20px;
}
.share-timetree >li {
	position: relative;
	display: block;
	width: 100%;
	padding:20px 0px 0px 32px;
}
.share-timetree >li:before {
	content: "";
	position: absolute;
	left: 20px;
	top:0;
	bottom:0;
	width: 3px;
	background: #f26c4f;
	z-index:1;
}
.share-timetree >li:after {
	content: "";
	position: absolute;
	left: 16px;
	top: 60px;
	width: 8px;
	height: 8px;
	border: solid 2px #007aff;
	border-radius: 50%;
	margin-top: -5px;
	z-index: 1;
	background:#fff;
}

.record-border{height:85px;border: solid #DFDFDF 1px;border-radius: 5px;}
.record-img{float: right;}
#record-title{margin-left: -41px;font-size: 12px;margin-top: 30px !important}
#record-title1{margin-left: -38px;font-size:12px;margin-top: 30px !important}
.timetree-txt{height:85px;border: solid #C0C0C0 1px;border-radius: 5px;padding-top: 10px;}
.dashed li:before {
	content: "";
	position: absolute;
	left: 100%;
	top:15px;
	bottom:20px;
	z-index:1;
}
.dashed li:last-child:before {
	display: none
}

