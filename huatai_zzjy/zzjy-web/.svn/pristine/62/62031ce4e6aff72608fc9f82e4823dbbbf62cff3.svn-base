<!-- 			  T2.TARGET_CODE -->
<!-- 			FROM HT_PLATE_TWO_TAR_TB T2 -->
<!-- 			WHERE T2.PTTG_ID IN(SELECT -->
<!-- 					      T3.PTTG_ID -->
<!-- 					    FROM HT_PLATE_TWO_TAR_GRO_TB T3 -->
<!-- 					    WHERE T3.PLATE_ID = #{blockId} -->
<!-- 						AND T3.OP_TYPE <![CDATA[ <> ]]> 'D') -->
<!-- 		    AND T2.OP_TYPE <![CDATA[ <> ]]> 'D') T4 LEFT JOIN HT_TARGET_TB T5 ON T4.TARGET_CODE = T5.TARGET_CODE WHERE T5.OP_TYPE <![CDATA[ <> ]]> 'D' -->
<!-- 			ORDER BY T4.TARGET_CODE) T8 -->
<!-- 	JOIN HT_TAR_GRO_DIM_TB T10 ON T8.TARGET_CODE = T10.TARGET_CODE -->
<!-- 	JOIN HT_GRO_DIM_DETAIL_TB T12 ON T10.GDD_ID = T12.GDD_ID -->
<!-- 	WHERE -->
<!-- 		T12.GRO_DIM_CODE = #{groupbyDate} -->
<!-- 	AND T12.OP_TYPE <![CDATA[ <> ]]>  'D' -->
<!-- 	AND T10.OP_TYPE <![CDATA[ <> ]]>  'D' -->
	SELECT
	T8.TARGET_CODE,
	T8.TARGET_NAME
FROM
	(
		SELECT
			T6.TARGET_CODE,
			T6.TARGET_NAME,
			MIN(T10.GRO_DIM_CODE) AS MIN_NUM
		FROM
			(
				SELECT
					T4.TARGET_CODE AS TARGET_CODE,
					T5.TARGET_NAME AS TARGET_NAME
				FROM
					(
						SELECT
							T1.TARGET_CODE AS TARGET_CODE
						FROM
							HT_PLATE_ONE_TAR_TB T1
						WHERE
							T1.PLATE_ID = #{blockId}
						AND T1.OP_TYPE <![CDATA[ <> ]]> 'D'
						UNION
							SELECT
								T2.TARGET_CODE
							FROM
								HT_PLATE_TWO_TAR_TB T2
							WHERE
								T2.PTTG_ID IN (
									SELECT
										T3.PTTG_ID
									FROM
										HT_PLATE_TWO_TAR_GRO_TB T3
									WHERE
										T3.PLATE_ID = #{blockId}
									AND T3.OP_TYPE <![CDATA[ <> ]]> 'D'
								)
							AND T2.OP_TYPE <![CDATA[ <> ]]> 'D'
					) T4
				LEFT JOIN HT_TARGET_TB T5 ON T4.TARGET_CODE = T5.TARGET_CODE 
				WHERE
					T5.OP_TYPE <![CDATA[ <> ]]> 'D'
				ORDER BY
					T4.TARGET_CODE
			) T6
		LEFT JOIN HT_TAR_GRO_DIM_TB T7 ON T6.TARGET_CODE = T7.TARGET_CODE
		AND T7.OP_TYPE <![CDATA[ <> ]]> 'D'
		LEFT JOIN (
			SELECT
				GDD_ID,
				GRO_DIM_CODE
			FROM
				HT_GRO_DIM_DETAIL_TB
			WHERE
				GD_ID IN (
					SELECT
						GD_ID
					FROM
						HT_GRO_DIM_TB
					WHERE
						GRO_DIM_TYPE_CODE = 'ORG_GROUP'
				)
		) T10 ON T7.GDD_ID = T10.GDD_ID
		GROUP BY
			T6.TARGET_CODE
	) T8
	JOIN HT_TAR_GRO_DIM_TB T10 ON T8.TARGET_CODE = T10.TARGET_CODE
	JOIN HT_GRO_DIM_DETAIL_TB T12 ON T10.GDD_ID = T12.GDD_ID
	WHERE T12.GRO_DIM_CODE =  #{groupbyDate}
	AND T12.OP_TYPE <![CDATA[ <> ]]>  'D'
	AND T10.OP_TYPE <![CDATA[ <> ]]>  'D'
    AND MIN_NUM <![CDATA[ < ]]> #{roleCate}
  </select>
